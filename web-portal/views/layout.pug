    title #{title}
    block headeradds
  body
    div(id="page-container")
      div(id="content-wrap")
        .container
          div(id="page-header")
            img(src="/images/Interlisp-D-Online.png", alt="Interlisp-D logo", width="300", height="150")
          block content
      footer(id="footer").
